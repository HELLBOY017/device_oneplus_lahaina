# OnePlus 9 CN/IN/EU/NA
on fs && property:ro.boot.prjname=19825
    mount overlay overlay /odm/etc lowerdir=/odm/oneplus9/etc:/odm/etc
    mount overlay overlay /odm/etc/audio/sku_lahaina lowerdir=/odm/oneplus9/etc/audio/sku_lahaina:/odm/etc/audio/sku_lahaina
    mount overlay overlay /odm/etc/acdbdata lowerdir=/odm/oneplus9/etc/acdbdata:/odm/etc/acdbdata
    mount overlay overlay /odm/etc/bt lowerdir=/odm/oneplus9/etc/bt:/odm/etc/bt
    mount overlay overlay /odm/etc/lvimfs_params lowerdir=/odm/oneplus9/etc/lvimfs_params:/odm/etc/lvimfs_params
    mount overlay overlay /odm/etc/init lowerdir=/odm/oneplus9/etc/init:/odm/etc/init
    mount overlay overlay /odm/etc/camera lowerdir=/odm/oneplus9/etc/camera:/odm/etc/camera
    mount overlay overlay /odm/etc/sensor lowerdir=/odm/oneplus9/etc/sensor:/odm/etc/sensor
    mount overlay overlay /odm/firmware lowerdir=/odm/oneplus9/firmware:/odm/firmware
    mount overlay overlay /odm/lib64/camera lowerdir=/odm/oneplus9/lib64/camera:/odm/lib64/camera
    mount overlay overlay /odm/vendor/firmware lowerdir=/odm/oneplus9/vendor/firmware:/odm/vendor/firmware

